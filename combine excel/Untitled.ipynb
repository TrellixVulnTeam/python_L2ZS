    "import pandas as pd\n",
    "import numpy as np\n",
    "import glob\n",
    "\n",
    "file=glob.glob(\"sales*.xlsx\")\n",
    "\n",
    "all_data = pd.DataFrame()\n",
    "for f in file:\n",
    "    df = pd.read_excel(f)\n",
    "    all_data = all_data.append(df,ignore_index=True)\n",
    "\n",
    "all_data.head(2)"
